<h1>Tournament {{date}}</h1>
<table class="details">
  <tr><td>Total players:</td><td>{{playersCount}}</td></tr>
  <tr><td>Total rebuys:</td><td>{{rebuys}}</td></tr>
  <tr><td>Buyin:</td><td>{{buyin}}</td></tr>
</table>
<div class="wrapper">
  <table>
    <tr>
      <th>Rank</th>
      <th>Player</th>
      <th>Points</th>
      <th>Prize</th>
      <th>Rebuys</th>
      <th>Bounty</th>
    </tr>
    {{#each players}}
      <tr>
        <td>{{this.ranking}}</td>
        <td class="name"><a data-animation="slideInRTL" href="/?view=profile&season_id={{../season_id}}&player_id={{this.id}}">{{this.name}}</a></td>
        <td>{{this.points}}</td>
        <td>{{this.prize}}</td>
        <td>{{this.rebuys}}</td>
        <td>{{this.bounty}}</td>
      </tr>
    {{/each}}
  </table>
</div>
