<h1>{{player_name}}
  {{#ifEquals ranking 1}}ðŸ¥‡{{/ifEquals}}
  {{#ifEquals ranking 2}}ðŸ¥ˆ{{/ifEquals}}
  {{#ifEquals ranking 3}}ðŸ¥‰{{/ifEquals}}
</h1>
<table class="details">
  <tr><td>{{seasonName}} ranking:</td><td>{{ranking}} </td></tr>
  <tr><td>Total points:</td><td>{{points}}</td></tr>
  <tr><td>Total rebuys:</td><td>{{rebuys}}</td></tr>
  <tr><td>Total games:</td><td>{{gamesCount}}</td></tr>
</table>
<div class="wrapper-chart">
    <canvas id="chart"></canvas>
</div>
<div class="wrapper">
  <table>
    <tr>
      <th>Game</th>
      <th>Points</th>
      <th>Ranking</th>
      <!--<th>Players</th>-->
      <th>Rebuys</th>
    </tr>
    {{#each results}}
      <tr>
        <td><a data-animation="slideInRTL" href="/?view=tournament&tournament_id={{this._id}}">{{this.date}}</a></td>
        <td>{{this.points}}</td>
        <td>{{this.ranking}}</td>
        <!--<td>{{this.players}}</td>-->
        <td>{{this.rebuys}}</td>
      </tr>
    {{/each}}
  </table>
</div>
