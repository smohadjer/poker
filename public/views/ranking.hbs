<h1>Ranking {{seasonName}}</h1>
<div class="wrapper">
  <table>
    <tr>
        <th>Rank</th>
        <th>Player</th>
        <th>Points</th>
        <th>Games</th>
        <!--<th>Rebuys</th>-->
    </tr>
    {{#each players}}
      <tr>
          <td>{{inc @index}}</td>
          <td class="name">
            {{#if ../seasonId}}
            <a data-animation="slideInRTL" href="/?view=profile&season={{../seasonId}}&player_id={{this.id}}">{{this.name}}</a>
              {{#ifEquals @index 0 }}ðŸ¥‡{{/ifEquals}}
              {{#ifEquals @index 1 }}ðŸ¥ˆ{{/ifEquals}}
              {{#ifEquals @index 2 }}ðŸ¥‰{{/ifEquals}}
            {{else}}
             <a data-animation="slideInRTL" href="/?view=profile&player_id={{this.id}}">{{this.name}}</a>
              {{#ifEquals @index 0 }}ðŸ¥‡{{/ifEquals}}
              {{#ifEquals @index 1 }}ðŸ¥ˆ{{/ifEquals}}
              {{#ifEquals @index 2 }}ðŸ¥‰{{/ifEquals}}
            {{/if}}
          </td>
          <td>{{this.points}}</td>
          <td>{{this.games}}</td>
          <!--<td>{{this.rebuys}}</td>-->
      </tr>
    {{/each}}
  </table>
</div>
